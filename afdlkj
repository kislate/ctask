[32m+[m[32m// kmpç®—æ³•åŒ¹é…[m
[32m+[m[32mint k = 0, m = 0, cnt = 0;[m
[32m+[m[32m// å­˜å‚¨æ–°ä¸²çš„æ•°ç»„[m
[32m+[m[32mchar *newStr = (char *)malloc(1000* sizeof(char));[m
[32m+[m[32mwhile(k < S.length)[m
[32m+[m[32m{[m
[32m+[m[32m    if(m == T.length) // å…¨åŒ¹é…æˆåŠŸ[m
[32m+[m[32m    {[m
[32m+[m[32m        for(int i = 0; i < V.length; i++) newStr[cnt++] = V.ch[i];[m
[32m+[m[32m        m = 0;[m
[32m+[m[32m    }[m
[32m+[m[32m    if(S.ch[k] == T.ch[m]) k++, m++; // å•åŒ¹é…æˆåŠŸ, åŒ¹é…æ•°åŠ ä¸€,ç»§ç»­åŒ¹é…ä¸‹ä¸€ä¸ªå­—ç¬¦[m
[32m+[m[32m    else{[m
[32m+[m[32m        if(m == 0) newStr[cnt++] = S.ch[k++]; // å•åŒ¹é…å¤±è´¥, kåŠ ä¸€, mä¸å˜[m
[32m+[m[32m        else[m
[32m+[m[32m        {[m
[32m+[m[32m            // æŠŠä¹‹å‰æ²¡åŠ çš„ç»™åŠ ä¸Šå»[m
[32m+[m[32m            for(int i = 0; i < (m - next[m]); i++) newStr[cnt++] = S.ch[k - m + i];[m
[32m+[m[32m            m = next[m];[m
[32m+[m[32m            if(m == -1) m = 0;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    // if(k == S.length) // æœªèƒ½æ­£ç¡®å¤„ç†ç»“æŸæ—¶å¦‚æœåˆšå¥½æœ‰éœ€è¦å¤„ç†çš„å­—ç¬¦ä¸²çš„æƒ…å†µ[m
[32m+[m[32m    // {[m
[32m+[m[32m    //     for(int i = k - m; i < S.length; i++) newStr[cnt++] = S.ch[i]; // æŠŠå‰©ä¸‹çš„å­—ç¬¦åŠ ä¸Šå»[m
[32m+[m[32m    //     break;[m
[32m+[m[32m    // }[m
[32m+[m[32m    if(k == S.length)[m
[32m+[m[32m    {[m
[32m+[m[32m        if(m == T.length)[m[41m [m
[32m+[m[32m        {[m
[32m+[m[32m            for(int i = 0; i < V.length; i++) newStr[cnt++] = V.ch[i];[m
[32m+[m[32m            m = 0;[m
[32m+[m[32m            break;[m
[32m+[m[32m        }[m
[32m+[m[32m        else[m
[32m+[m[32m        {[m
[32m+[m[32m            for(int i = k - m; i < S.length; i++) newStr[cnt++] = S.ch[i];[m
[32m+[m[32m            break;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m[32mfree(next);[m
[32m+[m[32mfree(S.ch);[m
[32m+[m[32mS.ch = newStr;[m
[32m+[m[32mS.length = cnt;[m
[32m+[m[32m/**************  end  *****************/[m
[32m+[m[32m}[m
[32m+[m[32mint main()[m
[32m+[m[32m{[m
[32m+[m[32mHString S[3];[m
[32m+[m[32mchar buf[100],i,j;[m
[32m+[m[32mfor(i=0;i<3;i++)[m
[32m+[m[32m{[m
[32m+[m	[32mfgets(buf,100,stdin);[m
[32m+[m	[32mS[i].length=strlen(buf)-1;  //-1æ˜¯å»æ‰å›è½¦ç¬¦å·[m
[32m+[m	[32mS[i].ch=(char *)malloc(S[i].length);[m
[32m+[m	[32mfor(j=0;j<S[i].length; j++)[m
[32m+[m		[32mS[i].ch[j]=*(buf+j);[m
[32m+[m[32m}[m
[32m+[m[32mReplace(S[0],S[1],S[2]);[m
[32m+[m[32mprintf("Sçš„é•¿åº¦ï¼š%d\n",S[0].length);[m
[32m+[m[32mfor(i=0;i<S[0].length;i++)[m
[32m+[m	[32mputchar(S[0].ch[i]);[m
[32m+[m[32mreturn 1;[m
